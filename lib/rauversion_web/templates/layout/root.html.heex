            new window.ChaskiqMessengerEncrypted({
              domain: 'https://app.chaskiq.io',
              ws:  'wss://app.chaskiq.io/cable',
              app_id: "<%= System.get_env("CHASKIQ_APP_ID") %>",
              //data: "YOUR_ENCRYPTED_JWE_DATA",
              //lang: "USER_LANG_OR_DEFAULTS_TO_BROWSER_LANG" 
            })
          }
        })(document,"script");
      </script>
    <% end %>


